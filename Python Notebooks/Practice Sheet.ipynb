    "import math\n",
    "def distance(cord):\n",
    "    (x1,y1), (x2,y2) = cord\n",
    "    dx =x2 - x1\n",
    "    dy = y2 - y1\n",
    "    sumtotal = math.sqrt(dx**2+dy**2)\n",
    "    return sumtotal"
   ]
  },
